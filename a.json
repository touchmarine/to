{
	"Elements": {
		"root": {
			"Type": "container",
			"Templates": {
				"html": "<html>\n<body>\n{{{content}}}\n</body>\n</html>\n"
			}
		},
		"attributes": {
			"Type": "verbatimWalled",
			"Delimiter": "!",
			"Templates": {
				"html": ""
			}
		},
		"textBlock": {
			"Type": "leaf",
			"Templates": {
				"html": "<span {{{attributes}}}>{{{content}}}</span>"
			}
		},
		"text": {
			"Type": "text",
			"Templates": {
				"html": "{{content}}"
			}
		},

		"Title": {
			"Type": "hanging",
			"Delimiter": "=",
			"Templates": {
				"html": "<h1 {{{attributes}}}>\n\t{{{content}}}\n</h1>\n"
			}
		},
		"NumberedHeading": {
			"Type": "rankedHanging",
			"Delimiter": "#",
			"Templates": {
				"html": "<h{{rank}} {{{attributes}}}>\n\t{{{content}}}\n</h{{rank}}>\n"
			}
		},
		"Paragraph": {
			"Type": "paragraph",
			"Option": "leaf",
			"Templates": {
				"html": "<p {{{attributes}}}>\n\t{{{content}}}\n</p>\n"
			}
		},
		"StickyAttributes": {
			"Type": "sticky",
			"Element": "attributes",
			"Templates": {
				"html": "{{{stickyTarget}}}"
			}
		},
		"TableOfContents": {
			"Type": "hanging",
			"Delimiter": ".toc",
			"Templates": {
				"html": "TOC\n{{{content}}}\n"
			}
		}
	},
	"Aggregates": {
		"headings": {
			"Type": "sequentialNumber",
			"Elements": ["NumberedHeading"] 
		},
		"NumberedHeadings": {
			"Type": "sequentialNumber",
			"Elements": ["NumberedHeading"] 
		}
	}
}
