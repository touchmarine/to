block = blockquote / paragraph / lines

listItem = "-" [block] [offsideBody]

offsideBody = LINEFEED INDENT [block] DEDENT

blockquote = BEGINBQUOTE [block] *(LINEFEED [block]) ENDWALLED
BEGINBQUOTE = ">" ; at start of line or another block

paragraph = BEGINPARA [block] *(LINEFEED [block]) ENDWALLED
BEGINPARA = "|" ; at start of line or another block

ENDWALLED = ; line starting with another or no block delimiter

lines = line *(LINEFEED line)
line  = 1*inline

inline = TEXT
TEXT = ; anything except inline delimiters, comments, or LINEFEEDs
